#=============================== TUTORIALS ===============================
#----------------------------- TUTORIAL SCENES ----------------------------
msgid "##movement_wasd## to move."
msgstr "##movement_wasd## pour se déplacer."

msgid "##movement_jump## to jump."
msgstr "##movement_jump## pour sauter."

msgid "Keep ##movement_jump## pressed to accelerate."
msgstr "Garder ##movement_jump## appuyé pour accélerer."

msgid "To accelerate even further :\n##movement_wa## + ##mouse_strafe_left##\nor\n##movement_wd## + ##mouse_strafe_right##."
msgstr "pour accélerer encore plus :\n##movement_wa## + ##mouse_strafe_left##\nou\n##movement_wd## + ##mouse_strafe_right##."

msgid "##movement_w## + ##mouse_strafe_left_right## mid air to keep the direction."
msgstr "##movement_w## + ##mouse_strafe_left_right## en l'air pour controler sa direction."

msgid "##restart_last_cp## to respawn at checkpoint.\n##restart## to restart."
msgstr "##restart_last_cp## pour réapparaitre au checkpoint.\n##restart## pour recommencer." 

msgid "##movement_slide## to slide."
msgstr "##movement_slide## pour glisser." 

msgid "##movement_jump## while sliding to boost jump."
msgstr "##movement_jump## pendant une glissade pour faire un saut accéléré." 

msgid "##movement_slide## to wallride\n##movement_jump## in wallride to jump."
msgstr "##movement_slide## pour wallride\n##movement_jump## en wallride pour sauter."

msgid "##shoot## to ##rocket_icon##."
msgstr "##shoot## pour ##rocket_icon##."

msgid "##rocket_icon## at feet to ##rocket_jump_icon##."
msgstr "##rocket_icon## aux pieds pour ##rocket_jump_icon##."

msgid "##rocket_jump_icon## on the wall to climb."
msgstr "##rocket_jump_icon## sur le mur pour escalader."

msgid "##rocket_icon## + ##boost_pad## to increase the force."
msgstr "##rocket_icon## + ##boost_pad## pour plus de puissance."
 
